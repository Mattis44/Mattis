---
import Photo from "../../assets/photo.jpeg";
---

<div
    class="flex w-full justify-end items-center align-middle min-h-[60px] h-[60px] border-b border-(--border-color)"
>
    <div
        class="w-10 h-10 rounded-full overflow-hidden border-2 border-(--header-border-color) mx-4"
    >
        <img
            src={Photo.src}
            alt="User Photo"
            class="w-full h-full object-cover"
        />
    </div>
</div>
